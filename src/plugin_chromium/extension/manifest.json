{
  "manifest_version": 3,
  "name": "LVT - Live Visual Tree",
  "version": "1.0.0",
  "description": "Exposes the DOM tree of browser tabs to the LVT CLI tool for AI-powered UI inspection.",
  "key": "",
  "permissions": [
    "debugger",
    "tabs",
    "nativeMessaging",
    "activeTab"
  ],
  "background": {
    "service_worker": "service-worker.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "minimum_chrome_version": "110"
}
